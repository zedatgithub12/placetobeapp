{
  "expo": {
    "name": "Place to be Ethiopia",
    "slug": "placetobe",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "com.afromina.placetobe",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.afromina.placetobe",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ],
      "@react-native-google-signin/google-signin"
    ],
    "description": "p2b mobile app published from expo server",
    "githubUrl": "https://github.com/afromina-placetobe/mobile-app",
    "extra": {
      "eas": {
        "projectId": "d6fb43a9-8f68-482e-842e-f995d73fa2a6"
      }
    }
  }
}
